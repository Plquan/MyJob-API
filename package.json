    "build": "tsc",
    "start": "node dist/server.js",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:run": "npm run typeorm migration:run -- -d src/ormconfig.ts",
    "migration:revert": "npm run typeorm migration:revert -- -d src/ormconfig.ts",
    "schema:sync": "npm run typeorm schema:sync -- -d src/ormconfig.ts",
    "migration:show": "npm run typeorm migration:show -- -d src/ormconfig.ts",
    "migration:generate": "npm run typeorm migration:generate ./src/migrations/Migration -- -d src/ormconfig.ts",
    "migration:create": "npm run typeorm migration:create -- -d src/ormconfig.ts"
